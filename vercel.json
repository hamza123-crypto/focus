{
  "version": 3,
  "builds": [
    {
      "src": "server/index-dev.ts",
      "use": "@vercel/node",
      "config": { "runtime": "nodejs20.x" }
    },
    {
      "src": "client/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server/index-dev.ts"
    }
  ]
}
