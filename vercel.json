{
  "version": 2,
  "builds": [
    {
      "src": "server/index-dev.ts",
      "use": "@vercel/node"
    },
    {
      "src": "client/**/*",
      "use": "@vercel/static-build",
      "config": { "distDir": "client/dist" }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/server/index-dev.ts" }
  ]
}
